<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ke.ac.rvtti.incubator.vote.vote" >
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ke.ac.rvtti.incubator.vote.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ke.ac.rvtti.incubator.vote.voter.RegisterActivity"
            android:label="@string/app_name">
            </activity>
     <activity android:name="ke.ac.rvtti.incubator.vote.choises.ChoiceActivity"
         android:label="@string/app_name">
         </activity>
        <activity android:name="ke.ac.rvtti.incubator.vote.voter.VoterAuthActivity"
            android:label="@string/app_name">
     </activity>
    </application>

</manifest>
